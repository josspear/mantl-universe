{
    "id": "dreg",
    "cpus": 1,
    "mem": 512,
    "instances": 1,
    "container": {
        "docker": {
            "image": "registry:2.3.1"
        },
        "type": "DOCKER"
    },
    "env": {
        "SETTINGS_FLAVOR": "local",
        "STORAGE_PATH": "/docker_storage"
    },
    "constraints": [
        [
            "hostname",
            "UNIQUE"
        ]
    ]
}
